    "@types/marked": "^2.0.3",
    "@types/react-reconciler": "^0.26.1",
    "css-loader": "^5.2.6",
    "dotenv": "^8.6.0",
    "eslint": "^7.27.0",
    "eslint-import-resolver-typescript": "^2.4.0",
    "react-reconciler": "^0.26.2",
    "style-loader": "^2.0.0",
    "stylus": "^0.54.8",
    "stylus-loader": "^6.0.0",
    "svgo": "^2.3.0",
    "ts-loader": "^9.2.2",
    "tsconfig-paths": "^3.9.0",
    "tsconfig-paths-webpack-plugin": "^3.5.1",
    "typescript": "^4.3.2",
    "webpack": "^5.38.1",
    "webpack-cli": "^4.7.0",
    "webpack-dev-server": "^3.11.2",
    "webpack-userscript": "^2.5.8"
  },
  "dependencies": {
    "dompurify": "^2.2.8",
    "easymde": "^2.15.0",
    "lodash": "^4.17.21",
    "marked": "^2.0.6"
  }
